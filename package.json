{
  "name": "iter-tools",
  "version": "6.2.3",
  "description": "The iterable toolbox",
  "main": "es5/index",
  "module": "es5/index.mjs",
  "jsnext:main": "es5/index.mjs",
  "typings": "es5/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test:js": "jest",
    "test:types": "tsc",
    "test": "run-s test:*",
    "benchmarks": "npm run clean && npm run build:es2018-cjs && node benchmarks",
    "coverage:badges": "jest-coverage-badges",
    "coverage": "npm run test:js -- --coverage --coverageDirectory=coverage && npm run coverage:badges",
    "clean": "rimraf es5 es2015 es2018",
    "generate": "node ./generate",
    "generate:watch": "node ./generate -w",
    "build": "run-s clean generate lint build:*",
    "build:es5": "cross-env BABEL_ENV=es babel --config-file ./babel-es5.config.js --keep-file-extension -x .mjs src -d es5",
    "build:es5-cjs": "cross-env BABEL_ENV=cjs babel --config-file ./babel-es5.config.js -x .mjs src -d es5",
    "build:es2015": "cross-env BABEL_ENV=es babel --keep-file-extension --config-file ./babel-es2015.config.js -x .mjs src -d es2015",
    "build:es2015-cjs": "cross-env BABEL_ENV=cjs babel --config-file ./babel-es2015.config.js -x .mjs src -d es2015",
    "build:es2018": "cross-env BABEL_ENV=es babel --keep-file-extension --config-file ./babel-es2018.config.js -x .mjs src -d es2018",
    "build:es2018-cjs": "cross-env BABEL_ENV=cjs babel --config-file ./babel-es2018.config.js -x .mjs src -d es2018",
    "build:copyts": "copyfiles -f src/*.ts es5 && copyfiles -f src/*.ts es2015 && copyfiles -f src/*.ts es2018",
    "create:method": "node ./generate-yo method",
    "verify": "run-s generate format verify:* build test",
    "verify:git-clean": "node ./verify-git-clean.js",
    "format": "prettier --write 'src/**/*.{js,mjs,d.ts}' 'generate/**/*.js'",
    "lint": "run-s lint:*",
    "lint:ts": "tslint -c tslint.json -p . 'src/**/*.ts'",
    "lint:js": "eslint --ext .mjs,.js ./src ./generate ./benchmarks",
    "prepublishOnly": "npm run build",
    "precommit": "npm run lint"
  },
  "keywords": [
    "Iterator",
    "Generator",
    "Map",
    "Set",
    "map",
    "filter",
    "functional",
    "itertools",
    "tools",
    "utils",
    "async"
  ],
  "repository": "git@github.com:sithmel/iter-tools.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "contributors": [
    "Conrad Buck <conartist6@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@babel/runtime-corejs2": "^7.4.3",
    "babel-plugin-macros": "^2.6.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "little-ds-toolkit": "^1.1.0",
    "minimist": "^1.2.0",
    "recursive-readdir": "^2.2.2",
    "sane": "conartist6/sane#micromatch-watchman-parse-glob",
    "typescript-tuple": "^2.1.0",
    "typescript-union": "^0.1.0",
    "utility-types": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-syntax-async-generators": "^7.2.0",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/plugin-syntax-typescript": "^7.3.3",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/plugin-transform-typescript": "^7.5.0",
    "@babel/preset-env": "^7.4.3",
    "@types/jest": "^24.0.15",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.7.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-jest": "^24.6.0",
    "benchmark": "^2.1.4",
    "camelcase": "^5.3.1",
    "camelize": "^1.0.0",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.6",
    "decamelize": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "git-state": "^4.1.0",
    "husky": "^0.14.3",
    "jest": "^24.7.1",
    "jest-coverage-badges": "^1.0.0",
    "json": "^9.0.6",
    "loglevel": "^1.6.2",
    "md5": "^2.2.1",
    "micromatch": "^4.0.2",
    "microtime": "^2.1.8",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "package.root": "^1.0.3",
    "path-complete-extname": "^1.0.0",
    "picomatch": "^2.0.7",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2",
    "static-type-assert": "^3.0.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5",
    "yeoman-environment": "^2.4.0",
    "yeoman-generator": "^4.0.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}

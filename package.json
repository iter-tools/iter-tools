{
  "name": "iter-tools",
  "version": "4.0.0",
  "description": "The iterator toolbox",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "npm run build && mocha tests/**/*.js",
    "benchmarks": "node benchmarks",
    "watch": "npm run test -- -w",
    "lint": "eslint --fix --ext .mjs ./src ./tests ./benchmarks",
    "clean": "rm -rf lib/*.* && rm -rf lib/async/*.* && rm -rf es5/*.* && rm -rf es5/async/*.* && rm -rf package/*",
    "build": "npm run clean && cross-env BABEL_ENV=es5 babel -x \".mjs\" src -d es5 && cross-env BABEL_ENV=cjs babel -x \".mjs\" src -d lib && cp src/*.mjs src/*.d.ts lib && cp src/async/*.mjs src/async/*.d.ts lib/async",
    "dist": "npm run build && cp -R package.json lib/* es5 LICENSE README.md package/ && cd package && npm publish",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "keywords": [
    "iterators",
    "itertools",
    "generators"
  ],
  "repository": "git@github.com:sithmel/iter-tools.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "clone-regexp": "^1.0.1",
    "dequeue": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "measure-speed": "^1.0.0",
    "mocha": "^5.0.5",
    "renamer": "^0.6.1",
    "require-all": "^2.2.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
